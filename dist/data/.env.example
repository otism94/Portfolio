# All of these are required. Exception will be thrown if any are missing
SMTP_HOST='smtp.example.com'
SMTP_PORT=2525 # Site is configured for port 2525, 587 should be coming soon
SMTP_AUTH=true # Should always be true
SMTP_USER='example@email.com' # Usually an email address unless a third-party SMTP tool is used
SMTP_PASS='password123'
SMTP_FROM='example@email.com' # Needs to be your own email address, most likely the one receiving the email
MAILTO_EMAIL='example@email.com' # The email address that will receive messages
MAILTO_NAME='My Name'